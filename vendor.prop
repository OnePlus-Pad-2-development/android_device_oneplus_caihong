# Bluetooth
bluetooth.device.default_name=OnePlus Pad 2

# Display
vendor.display.enable_rc_support=1
debug.sf.ignore_hwc_physical_display_orientation=true
ro.surface_flinger.primary_display_orientation=ORIENTATION_270

# Graphics
debug.sf.auto_latch_unsignaled=0
debug.sf.disable_client_composition_cache=0
ro.surface_flinger.set_idle_timer_ms=60
ro.surface_flinger.set_touch_timer_ms=120

# USB
vendor.usb.product_string=OnePlus Pad 2
